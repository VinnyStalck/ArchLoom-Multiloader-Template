{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${home_page}",
    "sources": "${source_page}",
    "issues": "${issues_page}"
  },
  "license": "${mod_licence}",
  "icon": "assets/examplemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${mod_group}.${mod_id}.fabric.${mod_class}Fabric"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabric": "${fabric_loader_version_range}",
    "minecraft": "${fabric_minecraft_version_range}"
  }
}